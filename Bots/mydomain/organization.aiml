<!--机构测试-->
<category>
	<pattern>*研究所</pattern>
	<template>
		<think>
			<!-- 实体类型，和数据表名、字段名一致-->
		    <set name="type">organization</set>
		    <set name="lib">organizations</set>
		    <set name="targetfield">description</set>
		    <set name="queryfield">orgname</set>
		    <set name="value"><input index="1"/></set>
		</think>
		<random>
			<li><input index="1"/>: </li>
		</random>
	</template>
</category> 

